{% extends "base.html" %}
{% load mi_tags %}{% load url from future %}

{% block title %}All Math Insight pages{% endblock %}
{% block nolinksection %}{% endblock %}
{% block nopagenav %}{% endblock %}{% block nopagenavsl %}{% endblock %}

{% block content %}

<h3>List of all pages</h3>

<ul class="linklist">
  {% for page in page_list %}
  <li>{% intlink page %}{{ page.title }}{%endintlink%}<br/>
	      {{ page.description }}</li>
  {% endfor %}
</ul>
{% if is_paginated %}
<div>
  {% if page_obj.has_previous %}<a href="?page={{ page_obj.previous_page_number }}">{% endif %}&laquo; Previous{% if page_obj.has_previous %}</a>{% endif %}
  | Page {{ page_obj.number }} of {{ paginator.num_pages }} |
  {% if page_obj.has_next %}<a href="?page={{ page_obj.next_page_number }}">{% endif %}Next &raquo;{% if page_obj.has_next %}</a>{% endif %}
</div>
{% endif %}
{% endblock %}


{% block rightcolumn %}

<ul class="linklist">
<li><a href="{% url 'mi-newpages'%}">Newest pages</a></li>
<li><a href="{% url 'mi-appletlist' %}">List of all applets</a></li>
<li><a href="{% url 'mi-videolist' %}">List of all videos</a></li>
<li><a href="{% url 'mi-imagelist' %}">List of all images</a></li>
</ul>

{%endblock%}
