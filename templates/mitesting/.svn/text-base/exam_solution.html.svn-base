{% extends "base.html" %}
{% load testing_tags %}

{% block extrastyle %}
<link href="{{ STATIC_URL }}css/miexam.css" rel="stylesheet" type="text/css"/>
{% endblock %}
{% block title %}{{ the_assessment_name }}, version {{ version }} - Math Insight{%endblock%}

{% block content %}

<table class="examinfo"><tr><td>{{ course }}</td></tr>
<tr><td>{{ semester }}</td></tr>
<tr><td>{{ the_assessment_name }}, version {{ version }}</td></tr>
<tr><td>{{ assessment_date }}</td></tr>
</table>
<section class="exam_main">
<ol class="exam_solutions">
{% for solution in solution_list %}
{% comment %}{% if not forloop.first %}{% new_exam_page %}{% endif %}{% endcomment %}
<li class="solution">{% if solution.points %}({{solution.points}} points) {%endif%}{{ solution.solution_text }}</li>
{% endfor %}
</ol>
</section>

{% endblock %}

